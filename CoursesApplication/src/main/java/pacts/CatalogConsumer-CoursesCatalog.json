{
  "consumer": {
    "name": "CatalogConsumer"
  },
  "interactions": [
    {
      "description": "Get the Appium course details",
      "key": "6ddb1c20",
      "pending": false,
      "providerStates": [
        {
          "name": "Course Appium exist"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/getCourseByName/Appium"
      },
      "response": {
        "body": {
          "content": {
            "category": "mobile",
            "price": 44
          },
          "contentType": "application/json; charset=UTF-8",
          "encoded": false
        },
        "headers": {
          "Content-Type": [
            "application/json; charset=UTF-8"
          ]
        },
        "matchingRules": {
          "body": {
            "$.category": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.price": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            }
          },
          "header": {
            "Content-Type": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                }
              ]
            }
          }
        },
        "status": 200
      },
      "type": "Synchronous/HTTP"
    },
    {
      "description": "getting all courses details",
      "key": "da792bd6",
      "pending": false,
      "providerStates": [
        {
          "name": "courses exist"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/allCourseDetails"
      },
      "response": {
        "body": {
          "content": [
            {
              "price": 10
            },
            {
              "price": 10
            }
          ],
          "contentType": "application/json; charset=UTF-8",
          "encoded": false
        },
        "headers": {
          "Content-Type": [
            "application/json; charset=UTF-8"
          ]
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 2
                }
              ]
            },
            "$[*].price": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            }
          },
          "header": {
            "Content-Type": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                }
              ]
            }
          }
        },
        "status": 200
      },
      "type": "Synchronous/HTTP"
    }
  ],
  "metadata": {
    "pact-jvm": {
      "version": "4.3.6"
    },
    "pactSpecification": {
      "version": "4.0"
    }
  },
  "provider": {
    "name": "CoursesCatalog"
  }
}